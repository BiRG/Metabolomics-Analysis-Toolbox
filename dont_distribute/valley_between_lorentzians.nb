     RowBox[{"ddf", "[", "a", "]"}], "<", "0"}], "&&", 
    RowBox[{
     RowBox[{"df", "[", "a", "]"}], "\[Equal]", "0"}], "&&", 
    RowBox[{"g1", ">", "0"}], " ", "&&", 
