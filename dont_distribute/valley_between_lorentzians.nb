}], "Text",
 CellChangeTimes->{{3.5635362503469973`*^9, 3.563536306728595*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Reduce", "[", 
  RowBox[{
   RowBox[{"Not", "[", 
    RowBox[{"Exists", "[", 
     RowBox[{"a", ",", 
      RowBox[{
       RowBox[{
       "0", "<", "rmin", " ", "<", " ", "a", " ", "<", " ", "rmax", " ", "<", 
        "x1"}], "&&", 
       RowBox[{
        RowBox[{"df", "[", "a", "]"}], "\[Equal]", "0"}], "&&", 
       RowBox[{
        RowBox[{"ddf", "[", "a", "]"}], "<", "0"}], "&&", 
       RowBox[{"g1", ">", "0"}], " ", "&&", 
       RowBox[{"m1", ">", "0"}], "&&", 
       RowBox[{"m0", ">", "0"}]}]}], "]"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"rmin", ",", "rmax"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.563536341299246*^9, 3.563536518676072*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Reduce", "::", "\<\"nsmet\"\>"}], 
  RowBox[{
  ":", " "}], "\<\"\\!\\(\\*StyleBox[\\\"\\\\\\\"This system cannot be solved \
with the methods available to Reduce.\\\\\\\"\\\", \\\"MT\\\"]\\) \
\\!\\(\\*ButtonBox[\\\"\[RightSkeleton]\\\", ButtonStyle->\\\"Link\\\", \
ButtonFrame->None, ButtonData:>\\\"paclet:ref/Reduce\\\", ButtonNote -> \
\\\"Reduce::nsmet\\\"]\\)\"\>"}]], "Message", "MSG",
 CellChangeTimes->{3.56353652058997*^9}],

Cell[BoxData[
 RowBox[{"Reduce", "[", 
  RowBox[{
   RowBox[{
    SubscriptBox["\[ForAll]", "a"], 
    RowBox[{"!", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"0", "<", "rmin", "<", "a", "<", "rmax", "<", "x1"}], "&&", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          FractionBox[
           RowBox[{"2", " ", "a", " ", "m0"}], 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"1", "+", 
              SuperscriptBox["a", "2"]}], ")"}], "2"]]}], "+", 
         FractionBox[
          RowBox[{"2", " ", "g1", " ", "m1", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "a"}], "+", "x1"}], ")"}]}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["g1", "2"], "+", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"a", "-", "x1"}], ")"}], "2"]}], ")"}], "2"]]}], 
        "\[Equal]", "0"}], "&&", 
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"2", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             RowBox[{"3", " ", 
              SuperscriptBox["a", "2"]}]}], ")"}], " ", "m0"}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"1", "+", 
             SuperscriptBox["a", "2"]}], ")"}], "3"]], "-", 
         FractionBox[
          RowBox[{"2", " ", "g1", " ", "m1", " ", 
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["g1", "2"], "-", 
             RowBox[{"3", " ", 
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"a", "-", "x1"}], ")"}], "2"]}]}], ")"}]}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["g1", "2"], "+", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"a", "-", "x1"}], ")"}], "2"]}], ")"}], "3"]]}], "<", 
        "0"}], "&&", 
       RowBox[{"g1", ">", "0"}], "&&", 
       RowBox[{"m1", ">", "0"}], "&&", 
       RowBox[{"m0", ">", "0"}]}], ")"}]}]}], ",", 
   RowBox[{"{", 
    RowBox[{"rmin", ",", "rmax"}], "}"}]}], "]"}]], "Output",
 CellChangeTimes->{3.5635365205966587`*^9}]
}, Open  ]],

