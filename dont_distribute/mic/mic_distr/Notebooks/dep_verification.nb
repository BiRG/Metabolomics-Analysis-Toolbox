Cell[CellGroupData[{

Cell["Code", "Subsection",
 CellChangeTimes->{{3.541762719550054*^9, 3.541762719899864*^9}}],

Cell[TextData[{
 "I wrote a new implementation. Since ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " is such a different language, the bugs should be significantly different. \
I was also able to view ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "'s intermediate responses by hand and verify that they were what I \
expected. Thus, I can treat any discrepancies between the ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "  version and the Java version as being a problem."
}], "Text",
 CellChangeTimes->{{3.5417627351464977`*^9, 3.541762839141889*^9}}],

Cell["\<\
Code for the distance matrix - distances between all points in the list\
\>", "Text",
 CellChangeTimes->{{3.5417617268799763`*^9, 3.541761740498083*^9}}],

Cell[CellGroupData[{

Cell["distMat, rowMeans, colMeans", "Subsubsubsection",
 CellChangeTimes->{{3.5417629507605247`*^9, 3.541762960417426*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"distMat", "[", "l_List", "]"}], ":=", 
  RowBox[{"Map", "[", 
   RowBox[{
    RowBox[{"Function", "[", 
     RowBox[{"rowVal", ",", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{"Function", "[", 
         RowBox[{"colVal", ",", 
          RowBox[{"Abs", "[", 
           RowBox[{"rowVal", "-", "colVal"}], "]"}]}], "]"}], ",", "l"}], 
       "]"}]}], "]"}], ",", "l"}], "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.5416701789220457`*^9, 3.541670198801866*^9}, {
  3.5416702365266447`*^9, 3.541670296257236*^9}}],

Cell["Code for the row means of a distance matrix", "Text",
 CellChangeTimes->{{3.541761753099156*^9, 3.541761760234496*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"rowMeans", "[", "l_List", "]"}], ":=", 
  RowBox[{"Map", "[", 
   RowBox[{"Mean", ",", "l"}], "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.541670541350669*^9, 3.541670555625712*^9}}],

Cell["Code for the column means of a distance matrix", "Text",
 CellChangeTimes->{{3.541761762617085*^9, 3.541761769463174*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"colMeans", "[", "l_List", "]"}], ":=", 
  RowBox[{"Map", "[", 
   RowBox[{"Mean", ",", 
