Cell[CellGroupData[{

Cell["pasteRows, pasteCols, and Capital Letter Matrix", "Subsubsubsection",
 CellChangeTimes->{{3.541763032095242*^9, 3.541763046216226*^9}, 
   3.541763272263569*^9}],

Cell["\<\
Code for making a square matrix by repeating one row or one column\
\>", "Text",
 CellChangeTimes->{{3.5417041873035517`*^9, 3.541704202627614*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"pasteRows", "[", "oneRow_List", "]"}], ":=", 
  RowBox[{"Table", "[", 
   RowBox[{"oneRow", ",", 
    RowBox[{"{", 
     RowBox[{"Length", "[", "oneRow", "]"}], "}"}]}], "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.541704024467642*^9, 3.541704094713482*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"pasteCols", "[", "oneCol_List", "]"}], ":=", 
  RowBox[{"Transpose", "[", 
   RowBox[{"pasteRows", "[", "oneCol", "]"}], "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.541704121075036*^9, 3.541704158721526*^9}}],

Cell["\<\
Code for making a capital letter matrix (as in the Distance Covariance paper, \
the lower case letters were the original distance matrices and the capital \
letter matrices were an important substep in calculating the Distance \
Covariance)\
\>", "Text",
 CellChangeTimes->{{3.541761260938221*^9, 3.541761322415936*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"capMatrix", "[", "l_List", "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"dm", "=", 
      RowBox[{"distMat", "[", "l", "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"rowM", "=", 
         RowBox[{"pasteRows", "[", 
          RowBox[{"rowMeans", "[", "dm", "]"}], "]"}]}], ",", 
        RowBox[{"colM", "=", 
         RowBox[{"pasteCols", "[", 
          RowBox[{"colMeans", "[", "dm", "]"}], "]"}]}], ",", 
        RowBox[{"allMean", "=", 
         RowBox[{"Mean", "[", 
          RowBox[{"Flatten", "[", "dm", "]"}], "]"}]}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"meanM", "=", 
          RowBox[{"Map", "[", 
           RowBox[{
            RowBox[{"allMean", "&"}], ",", "dm", ",", 
            RowBox[{"{", "2", "}"}]}], "]"}]}], "}"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"dm", "-", "colM", "-", "rowM", "+", "meanM"}]}], 
       "\[IndentingNewLine]", "]"}]}], "]"}]}], "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.5417042419452333`*^9, 3.541704416643716*^9}, 
   3.5417044667423143`*^9}]
}, Open  ]],

