Cell["dCov (distance covariance) and dCor (distance correlation)", \
"Subsubsubsection",
 CellChangeTimes->{{3.541763251258451*^9, 3.541763263745844*^9}}],

Cell["Code for calculating the distance covariance", "Text",
 CellChangeTimes->{{3.5417613453379583`*^9, 3.541761354013194*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"dCov", "[", 
   RowBox[{"x_List", ",", "y_List"}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"a", "=", 
       RowBox[{"capMatrix", "[", "x", "]"}]}], ",", 
      RowBox[{"b", "=", 
       RowBox[{"capMatrix", "[", "y", "]"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Dot", "[", 
      RowBox[{
       RowBox[{"Flatten", "[", "a", "]"}], ",", 
       RowBox[{"Flatten", "[", "b", "]"}]}], "]"}], "/", 
     RowBox[{"Length", "[", 
      RowBox[{"Flatten", "[", "a", "]"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.5417111174449873`*^9, 3.541711212957878*^9}, {
  3.5417112700031757`*^9, 3.541711273490631*^9}, {3.541711329796556*^9, 
  3.541711346742935*^9}, {3.541711434465034*^9, 3.5417114454927673`*^9}}],

Cell["Code for calculating the distance correlation", "Text",
 CellChangeTimes->{{3.541761356838544*^9, 3.541761362371347*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"dCor", "[", 
   RowBox[{"x_List", ",", "y_List"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"dCov", "[", 
    RowBox[{"x", ",", "y"}], "]"}], "/", 
   RowBox[{"Sqrt", "[", 
    RowBox[{
     RowBox[{"dCov", "[", 
      RowBox[{"x", ",", "x"}], "]"}], 
     RowBox[{"dCov", "[", 
      RowBox[{"y", ",", "y"}], "]"}]}], "]"}]}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.5417114646597424`*^9, 3.541711504434102*^9}}]
}, Open  ]]
