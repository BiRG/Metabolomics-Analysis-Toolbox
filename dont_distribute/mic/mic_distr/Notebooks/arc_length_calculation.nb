 CellChangeTimes->{{3.541949663544489*^9, 3.541949668956917*^9}}],

Cell["\<\
Instead, I will use symmetry. The sine function is symmetric on the quarter \
period. So, the arc lengths should be the sum of the quarter period lengths \
times a scale factor.\
\>", "Text",
 CellChangeTimes->{{3.541949670652697*^9, 3.5419496772436533`*^9}, {
  3.5419497478922367`*^9, 3.5419497533336897`*^9}, {3.5419497884562073`*^9, 
  3.541949803008284*^9}, {3.5419498914554377`*^9, 3.541949916555449*^9}}],

Cell["First, the full period length", "Text",
 CellChangeTimes->{{3.541949921494989*^9, 3.5419499344886312`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"lengthForKPi", "[", "k_Integer", "]"}], ":=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"Sqrt", "[", 
     RowBox[{"1", "+", 
      RowBox[{
       FractionBox["1", "4"], " ", 
       SuperscriptBox["k", "2"], " ", 
       SuperscriptBox["\[Pi]", "2"], " ", 
       SuperscriptBox[
        RowBox[{"Cos", "[", 
         RowBox[{"k", " ", "\[Pi]", " ", "x"}], "]"}], "2"]}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.541949251867982*^9, 3.541949274581068*^9}, {
  3.541949537875865*^9, 3.54194957179119*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"N", "[", 
  RowBox[{"lengthForKPi", "[", "2", "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.541949577102647*^9, 3.541949611017359*^9}}],

Cell[BoxData["2.3048926613536906`"], "Output",
 CellChangeTimes->{3.54194961176757*^9}]
}, Open  ]],

Cell["Now the quarter period length", "Text",
 CellChangeTimes->{{3.5419499294915037`*^9, 3.541949940228128*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"N", "[", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"Sqrt", "[", 
     RowBox[{"1", "+", 
      RowBox[{
       FractionBox["1", "4"], " ", 
       SuperscriptBox["2", "2"], " ", 
       SuperscriptBox["\[Pi]", "2"], " ", 
       SuperscriptBox[
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[Pi]", " ", "x"}], "]"}], "2"]}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", 
      RowBox[{"1", "/", "4"}]}], "}"}]}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.541949620790347*^9, 3.5419496243503532`*^9}}],

Cell[BoxData["0.5762231653384227`"], "Output",
 CellChangeTimes->{3.541949625202565*^9}]
}, Open  ]],

Cell["Indeed the symmetry holds.", "Text",
 CellChangeTimes->{{3.5419499443891172`*^9, 3.541949973080607*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"2.3048926613536906`", "/", "0.5762231653384227`"}]], "Input",
 CellChangeTimes->{{3.541949632277025*^9, 3.541949636537158*^9}}],

Cell[BoxData["4.`"], "Output",
 CellChangeTimes->{3.541949637199561*^9}]
}, Open  ]],

Cell["Now, let's see whether there is a scale factor", "Text",
 CellChangeTimes->{{3.541950020386475*^9, 3.541950028424097*^9}, 
   3.541950058944517*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"lengthForKPi", "[", "4", "]"}], "/", 
  RowBox[{"lengthForKPi", "[", "2", "]"}]}]], "Input",
 CellChangeTimes->{{3.5419499818357573`*^9, 3.541949993488957*^9}, {
  3.541950031586485*^9, 3.541950035982706*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SqrtBox[
    FractionBox[
     RowBox[{"1", "+", 
      RowBox[{"4", " ", 
       SuperscriptBox["\[Pi]", "2"]}]}], 
     RowBox[{"1", "+", 
      SuperscriptBox["\[Pi]", "2"]}]]], " ", 
   RowBox[{"EllipticE", "[", 
    FractionBox[
     RowBox[{"4", " ", 
      SuperscriptBox["\[Pi]", "2"]}], 
     RowBox[{"1", "+", 
      RowBox[{"4", " ", 
       SuperscriptBox["\[Pi]", "2"]}]}]], "]"}]}], 
  RowBox[{"EllipticE", "[", 
   FractionBox[
    SuperscriptBox["\[Pi]", "2"], 
    RowBox[{"1", "+", 
     SuperscriptBox["\[Pi]", "2"]}]], "]"}]]], "Output",
 CellChangeTimes->{{3.541949990129032*^9, 3.5419499940830297`*^9}, 
   3.541950036866486*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FullSimplify", "[", "%", "]"}]], "Input",
 CellChangeTimes->{{3.541950072256605*^9, 3.541950080048904*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SqrtBox[
    RowBox[{"4", "-", 
     FractionBox["3", 
      RowBox[{"1", "+", 
       SuperscriptBox["\[Pi]", "2"]}]]}]], " ", 
   RowBox[{"EllipticE", "[", 
    FractionBox[
     RowBox[{"4", " ", 
      SuperscriptBox["\[Pi]", "2"]}], 
     RowBox[{"1", "+", 
      RowBox[{"4", " ", 
       SuperscriptBox["\[Pi]", "2"]}]}]], "]"}]}], 
  RowBox[{"EllipticE", "[", 
   FractionBox[
    SuperscriptBox["\[Pi]", "2"], 
    RowBox[{"1", "+", 
     SuperscriptBox["\[Pi]", "2"]}]], "]"}]]], "Output",
 CellChangeTimes->{{3.541950075153544*^9, 3.541950080612967*^9}}]
}, Open  ]],

Cell["Oops, no obvious scale factor.", "Text",
 CellChangeTimes->{{3.541950101693652*^9, 3.541950105671093*^9}, {
  3.541950154803993*^9, 3.541950156194268*^9}}],

Cell["\<\
Let's check to see whether something is hiding in the notation that gets \
revealed when we convert to numbers.\
\>", "Text",
 CellChangeTimes->{{3.541950158680192*^9, 3.541950181021181*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"N", "[", 
  RowBox[{
   RowBox[{"lengthForKPi", "[", "4", "]"}], "/", 
   RowBox[{"lengthForKPi", "[", "2", "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.54195021374866*^9, 3.541950216255783*^9}}],

