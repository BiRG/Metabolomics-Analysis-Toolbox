  RowBox[{
   RowBox[{
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"a", "+", 
       RowBox[{"x", 
        RowBox[{"(", 
         RowBox[{"b", "+", 
          RowBox[{"x", 
           RowBox[{"(", 
            RowBox[{"c", " ", "+", 
             RowBox[{"d", " ", "x"}]}], ")"}]}]}], ")"}]}]}], ",", "x"}], 
     "]"}], "\[Equal]", "0"}], ",", 
