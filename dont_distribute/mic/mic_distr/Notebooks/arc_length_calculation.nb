Notebook[{

Cell[CellGroupData[{
Cell["Arc Length Calculations", "Title",
 CellChangeTimes->{{3.5419440269467363`*^9, 3.5419440330369987`*^9}}],

Cell["Eric Moyer", "Subtitle",
 CellChangeTimes->{{3.541944038874807*^9, 3.541944041358968*^9}}],

Cell["28 March 2012", "Subsubtitle",
 CellChangeTimes->{{3.541944082263652*^9, 3.5419440858117857`*^9}}],

Cell["\<\
The functions that will be combined through union to create new functions \
need their arc-lengths calculated so that the samples are unformly \
distributed throughout all the points in the relations.\
\>", "Text",
 CellChangeTimes->{{3.541944093859522*^9, 3.54194414609636*^9}}],

Cell["\<\
Since I want to find the arc length when the function is confined to a box, \
this could be seen as the minimum of the arc lengths when the integral is \
taken over 0,1 for x and over 0,1 for y. For functions that are not \
invertible, I'll have to do a parametric version and find the version of the \
parameter for the last coordinate in the box.\
\>", "Text",
 CellChangeTimes->{{3.541945083667551*^9, 3.541945246558288*^9}}],

Cell[CellGroupData[{

Cell["Code", "Section",
 CellChangeTimes->{{3.541944334045814*^9, 3.541944334471053*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"arcL", "[", "f_", "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"dx", "=", 
      RowBox[{"D", "[", 
       RowBox[{"f", ",", "x"}], "]"}]}], "}"}], ",", 
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"1", "+", 
        RowBox[{
         RowBox[{"(", "dx", ")"}], "^", "2"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}], "]"}]}]], "Input",\

 CellChangeTimes->{{3.541943384804*^9, 3.541943384834371*^9}, {
   3.541943525618286*^9, 3.541943533185052*^9}, {3.5419436184374104`*^9, 
   3.5419436343870287`*^9}, {3.541943674027492*^9, 3.5419438010339108`*^9}, {
   3.541943835744556*^9, 3.5419438369285316`*^9}, {3.541943966359508*^9, 
   3.541943976627084*^9}, {3.5419449837920732`*^9, 3.5419449982042847`*^9}, 
   3.5419450663919077`*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell["Linear", "Section",
 CellChangeTimes->{{3.54194433875181*^9, 3.5419443404449377`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"arcL", "[", "x", "]"}]], "Input",
 CellChangeTimes->{{3.54194383902472*^9, 3.541943841052619*^9}}],

Cell[BoxData[
 SqrtBox["2"]], "Output",
 CellChangeTimes->{3.541943841497883*^9, 3.5419439786558657`*^9, 
  3.541945074689828*^9}]
}, Open  ]],

Cell[BoxData[
 RowBox[{
  RowBox[{"linArcL", "[", 
   RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
  RowBox[{"Min", "[", 
   RowBox[{
    RowBox[{"arcL", "[", 
     RowBox[{
      RowBox[{"a", " ", "x"}], "+", "b"}], "]"}], ",", 
    RowBox[{"arcL", "[", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "-", "b"}], ")"}], "/", "a"}], "]"}]}], "]"}]}]], "Input",\

 CellChangeTimes->{{3.541944761661137*^9, 3.541944776541687*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"linArcL", "[", 
  RowBox[{"a", ",", "b"}], "]"}]], "Input",
 CellChangeTimes->{{3.541944160404076*^9, 3.541944165072413*^9}, {
  3.5419446038329906`*^9, 3.5419446059536037`*^9}, {3.5419446988581963`*^9, 
  3.5419447081161327`*^9}, {3.541944786902529*^9, 3.541944789953013*^9}}],

Cell[BoxData[
 RowBox[{"Min", "[", 
  RowBox[{
   SqrtBox[
    RowBox[{"1", "+", 
     FractionBox["1", 
      SuperscriptBox["a", "2"]]}]], ",", 
   SqrtBox[
    RowBox[{"1", "+", 
     SuperscriptBox["a", "2"]}]]}], "]"}]], "Output",
 CellChangeTimes->{3.54194417235737*^9, 3.541944708705058*^9, 
  3.541944790479244*^9}]
}, Open  ]],

