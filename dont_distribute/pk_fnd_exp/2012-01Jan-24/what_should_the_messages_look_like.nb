Cell[TextData[{
 "I want to discover the equations for the messages I am using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Mu]", 
     RowBox[{
      SubscriptBox["f", "0"], "\[Rule]", 
      SubscriptBox["a", "0"]}]], "(", 
    SubscriptBox["a", "0"], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.53642018123238*^9, 3.5364202243156433`*^9}}],

Cell["In general:", "Text",
 CellChangeTimes->{{3.536420241391383*^9, 3.5364202434963703`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Mu]", 
     RowBox[{
      SubscriptBox["f", "0"], "\[Rule]", 
      SubscriptBox["a", "0"]}]], "(", 
    SubscriptBox["a", "0"], ")"}], "=", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{
      SubscriptBox["x", "0"], "-", 
      RowBox[{
       SubscriptBox["max", "0"], 
       RowBox[{"(", 
        SubscriptBox["a", "0"], ")"}]}]}], 
     RowBox[{
      SubscriptBox["x", "0"], "-", 
      RowBox[{
       SubscriptBox["min", "0"], 
       RowBox[{"(", 
        SubscriptBox["a", "0"], ")"}]}]}]], 
    RowBox[{
     RowBox[{"P", "(", 
      RowBox[{
       SubscriptBox["\[Aleph]", "0"], "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          SubscriptBox["x", "0"], "-", 
          SubscriptBox["\[Xi]", "0"]}], "|", 
         SubscriptBox["A", "0"]}], "=", 
        SubscriptBox["a", "0"]}]}], ")"}], 
     RowBox[{"P", "(", 
      RowBox[{
       SubscriptBox["\[CapitalXi]", "0"], "=", 
       SubscriptBox["\[Xi]", "0"]}], ")"}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[Xi]", "0"]}]}]}]}], TraditionalForm]], "Input",
 CellChangeTimes->{{3.536420562585451*^9, 3.536420567585888*^9}, {
  3.536420607923687*^9, 3.536420622461039*^9}, {3.536420687259556*^9, 
  3.536420784331135*^9}}],

Cell["Which I show on page two of my paper notes for today.", "Text",
