       RowBox[{"mean", " ", 
        RowBox[{"(", 
         RowBox[{"min", "-", "x0"}], ")"}]}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"mean", "-", "min"}], ")"}], "2"]], "+", 
      FractionBox[
       RowBox[{"min", " ", "x0"}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"mean", "-", "min"}], ")"}], "2"]], "+", 
      FractionBox[
       SuperscriptBox["\[Sigma]", "2"], 
       RowBox[{"2", " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"mean", "-", "min"}], ")"}], "2"]}]]}]], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"mean", " ", "min"}], "-", 
      SuperscriptBox["min", "2"], "-", 
      RowBox[{"mean", " ", "x0"}], "+", 
      RowBox[{"min", " ", "x0"}], "+", 
      SuperscriptBox["\[Sigma]", "2"], "-", 
      RowBox[{
       RowBox[{"Abs", "[", 
        RowBox[{
         RowBox[{"-", 
