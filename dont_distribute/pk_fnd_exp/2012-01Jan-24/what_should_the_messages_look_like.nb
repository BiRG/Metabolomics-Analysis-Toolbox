  3.5364212533566732`*^9}, {3.536421499712944*^9, 3.536421504755164*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"p\[Aleph]", "[", 
   RowBox[{"\[Alpha]_", ",", "min_", ",", "max_", ",", "mean_"}], "]"}], ":=", 
  RowBox[{"Which", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"min", "\[Equal]", 
     RowBox[{"-", "Infinity"}]}], ",", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"\[Alpha]", "<", "max"}], ",", 
      FractionBox[
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", 
         FractionBox[
          RowBox[{"max", "-", "\[Alpha]"}], 
          RowBox[{"max", "-", "mean"}]]}]], 
       RowBox[{"max", "-", "mean"}]], ",", "0"}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"max", "\[Equal]", "Infinity"}], ",", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"-", "min"}], "+", "\[Alpha]"}], ">", "0"}], ",", 
      FractionBox[
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", 
         FractionBox[
          RowBox[{"\[Alpha]", "-", "min"}], 
          RowBox[{"mean", "-", "min"}]]}]], 
       RowBox[{"mean", "-", "min"}]], ",", "0"}], "]"}], ",", 
    "\[IndentingNewLine]", "True", ",", 
    RowBox[{"1", "/", 
     RowBox[{"(", 
      RowBox[{"max", "-", "min"}], ")"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.536421004021291*^9, 3.5364211242383833`*^9}, {
