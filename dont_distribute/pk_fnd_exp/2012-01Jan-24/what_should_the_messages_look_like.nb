Cell["\<\
The first bin is the one which has no lower bound.  The message sent for the \
first bin is calculated from the message integral given at the beginning of \
this document.\
\>", "Text",
 CellChangeTimes->{{3.536423919704268*^9, 3.536424016631591*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"messageFirstBin", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"p\[Xi]", "[", 
      RowBox[{"\[Xi]0", ",", "\[Sigma]"}], "]"}], 
     RowBox[{"p\[Aleph]", "[", 
      RowBox[{
       RowBox[{"x0", "-", "\[Xi]0"}], ",", 
       RowBox[{"-", "Infinity"}], ",", "max", ",", "mean"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"\[Xi]0", ",", 
      RowBox[{"x0", "-", "max"}], ",", "Infinity"}], "}"}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{
      RowBox[{"max", ">", "mean"}], "&&", 
      RowBox[{"\[Sigma]", ">", "0"}], "&&", 
      RowBox[{
       RowBox[{"x0", "-", "\[Xi]0"}], "<", "max"}]}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.536423364199128*^9, 3.536423436583729*^9}, {
  3.536423571759205*^9, 3.536423578019264*^9}, {3.536423792281797*^9, 
  3.536423793289551*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      FractionBox[
       RowBox[{
        RowBox[{
         RowBox[{"-", "2"}], " ", 
         SuperscriptBox["max", "2"]}], "-", 
        RowBox[{"2", " ", "mean", " ", "x0"}], "+", 
        RowBox[{"2", " ", "max", " ", 
         RowBox[{"(", 
          RowBox[{"mean", "+", "x0"}], ")"}]}], "+", 
        SuperscriptBox["\[Sigma]", "2"]}], 
       RowBox[{"2", " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"max", "-", "mean"}], ")"}], "2"]}]]], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "max"}], "+", "mean"}], ")"}], " ", 
        RowBox[{"Abs", "[", 
         RowBox[{
          RowBox[{"-", "max"}], "+", "x0", "+", 
          FractionBox[
           SuperscriptBox["\[Sigma]", "2"], 
           RowBox[{"max", "-", "mean"}]]}], "]"}]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["max", "2"]}], "-", 
          RowBox[{"mean", " ", "x0"}], "+", 
          RowBox[{"max", " ", 
           RowBox[{"(", 
            RowBox[{"mean", "+", "x0"}], ")"}]}], "+", 
          SuperscriptBox["\[Sigma]", "2"]}], ")"}], " ", 
        RowBox[{"Erf", "[", 
         FractionBox[
          RowBox[{"Abs", "[", 
           RowBox[{
            RowBox[{"-", "max"}], "+", "x0", "+", 
            FractionBox[
             SuperscriptBox["\[Sigma]", "2"], 
             RowBox[{"max", "-", "mean"}]]}], "]"}], 
          RowBox[{
           SqrtBox["2"], " ", "\[Sigma]"}]], "]"}]}]}], ")"}]}], ")"}], "/", 
   RowBox[{"(", 
    RowBox[{"2", " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"max", "-", "mean"}], ")"}], "2"], " ", 
     RowBox[{"Abs", "[", 
      RowBox[{
       RowBox[{"-", "max"}], "+", "x0", "+", 
       FractionBox[
        SuperscriptBox["\[Sigma]", "2"], 
        RowBox[{"max", "-", "mean"}]]}], "]"}]}], ")"}]}]}]], "Output",
 CellChangeTimes->{3.536423446158572*^9, 3.536423583667395*^9, 
