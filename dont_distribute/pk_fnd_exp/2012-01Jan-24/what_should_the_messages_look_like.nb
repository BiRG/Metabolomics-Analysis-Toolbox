    RowBox[{"{", 
     RowBox[{"\[Xi]0", ",", 
      RowBox[{"-", "Infinity"}], ",", 
      RowBox[{"x0", "-", "min"}]}], "}"}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{
      RowBox[{"\[Sigma]", ">", "0"}], "&&", 
