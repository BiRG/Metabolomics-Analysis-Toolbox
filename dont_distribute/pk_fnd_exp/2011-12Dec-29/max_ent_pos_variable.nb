Cell["\<\
What is the maximum entropy distribution for known \[Mu] and \[Sigma] where x \
is positive?\
\>", "Title",
 CellChangeTimes->{{3.534158317254424*^9, 3.534158344729989*^9}}],

Cell["\<\
The Gaussian is the maximum entropy distribution where \[Mu] and \[Sigma] are \
fixed and no other restrictions are made on p(x).  What if p(x)=0 if x \
\[LessEqual] 0?\
\>", "Text",
 CellChangeTimes->{{3.534158346459646*^9, 3.534158428736278*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"h", "[", "p_", "]"}], ":=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"p", "[", "x", "]"}], "\[NotEqual]", "0"}], ",", 
      RowBox[{
       RowBox[{"p", "[", "x", "]"}], 
       RowBox[{"Log", "[", 
        RowBox[{"p", "[", "x", "]"}], "]"}]}], ",", "0"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}]], "Input",\

 CellChangeTimes->{{3.5341584664882803`*^9, 3.5341585624698563`*^9}, {
  3.5341589309781103`*^9, 3.534158945802339*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Mu]", "[", "p_", "]"}], ":=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"x", " ", 
     RowBox[{"p", "[", "x", "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}]], "Input",\

 CellChangeTimes->{{3.534158760093244*^9, 3.534158788181319*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"var", "[", "p_", "]"}], ":=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"x", "-", 
        RowBox[{"\[Mu]", "[", "p", "]"}]}], ")"}], "2"], 
     RowBox[{"p", "[", "x", "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}]], "Input",\

 CellChangeTimes->{{3.534158568432681*^9, 3.534158624121881*^9}, {
  3.5341588127012463`*^9, 3.5341588194311237`*^9}}],

Cell[BoxData[
 RowBox[{
