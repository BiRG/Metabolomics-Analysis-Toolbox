Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "1"}], "+", "\[Lambda]0", "-", 
     FractionBox[
      SuperscriptBox["\[Lambda]1", "2"], 
      RowBox[{"4", " ", "\[Lambda]2"}]], "+", 
     RowBox[{"\[Lambda]1", " ", "\[Mu]"}]}]], " ", 
