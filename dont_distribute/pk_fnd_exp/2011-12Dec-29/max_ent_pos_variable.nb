Cell[CellGroupData[{
Cell["\<\
What is the maximum entropy distribution for known \[Mu] and \[Sigma] where x \
is positive?\
\>", "Title",
 CellChangeTimes->{{3.534158317254424*^9, 3.534158344729989*^9}}],

Cell[TextData[{
 "Here p[x] is the maximum entropy pdf of the distribution where we know that \
x is non-negative and we know the mean \[Mu] and variance ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Sigma]", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ".  The \[Lambda] terms are the lagrange multipliers.  \[Lambda]0 and \
\[Lambda]1 will be functions only of \[Mu].  \[Lambda]2 will also be a \
function of \[Sigma].  (\[Mu] appears in the constraint equations for \
\[Lambda]0 and \[Lambda]1.  \[Sigma] appears in the \[Lambda]2 constraint."
}], "Text",
 CellChangeTimes->{{3.534171427168804*^9, 3.5341714357657423`*^9}, {
  3.534171541586204*^9, 3.534171567376177*^9}, {3.53417168660343*^9, 
  3.534171809329925*^9}, {3.534171845141198*^9, 3.534171872430586*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"p", "[", "x_", "]"}], ":=", 
  RowBox[{"Exp", "[", 
   RowBox[{
    RowBox[{"-", "1"}], "+", "\[Lambda]0", "+", 
    RowBox[{"\[Lambda]1", " ", "x"}], " ", "+", " ", 
    RowBox[{"\[Lambda]2", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"x", "-", "\[Mu]"}], ")"}], "2"]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.534169544149577*^9, 3.5341695614336967`*^9}}],

Cell["Here, I apply the first constraint", "Text",
 CellChangeTimes->{{3.53417194007183*^9, 3.5341719448077507`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"totalArea", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"p", "[", "x", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "Infinity"}], "}"}], ",", 
    RowBox[{"Assumptions", "\[Rule]", " ", 
     RowBox[{
      RowBox[{"\[Lambda]2", "<", "0"}], "&&", 
      RowBox[{"\[Lambda]1", "<", "0"}]}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.534169331354871*^9, 3.5341693415526943`*^9}, {
   3.5341693832545*^9, 3.534169468762328*^9}, 3.534169502956249*^9, {
   3.5341695363015757`*^9, 3.5341695564136143`*^9}, {3.5341695899571943`*^9, 
   3.534169614840248*^9}, {3.5341697002164288`*^9, 3.534169706976192*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "1"}], "+", "\[Lambda]0", "-", 
     FractionBox[
      SuperscriptBox["\[Lambda]1", "2"], 
      RowBox[{"4", " ", "\[Lambda]2"}]], "+", 
     RowBox[{"\[Lambda]1", " ", "\[Mu]"}]}]], " ", 
   SqrtBox["\[Pi]"], " ", 
   RowBox[{"(", 
    RowBox[{"1", "+", 
     RowBox[{"Erf", "[", 
      FractionBox[
       RowBox[{"\[Lambda]1", "-", 
        RowBox[{"2", " ", "\[Lambda]2", " ", "\[Mu]"}]}], 
       RowBox[{"2", " ", 
        SqrtBox[
         RowBox[{"-", "\[Lambda]2"}]]}]], "]"}]}], ")"}]}], 
  RowBox[{"2", " ", 
   SqrtBox[
    RowBox[{"-", "\[Lambda]2"}]]}]]], "Output",
 CellChangeTimes->{3.534169622617669*^9, 3.534169708279646*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"InputForm", "[", "totalArea", "]"}]], "Input",
 CellChangeTimes->{{3.534170145244231*^9, 3.534170156187831*^9}}],

Cell["\<\
(E^(-1 + \[Lambda]0 - \[Lambda]1^2/(4*\[Lambda]2) + \[Lambda]1*\[Mu])*Sqrt[Pi]*
  (1 + Erf[(\[Lambda]1 - 2*\[Lambda]2*\[Mu])/(2*Sqrt[-\[Lambda]2])]))/
 (2*Sqrt[-\[Lambda]2])\
\>", "Output",
 CellChangeTimes->{3.534170156609028*^9}]
}, Open  ]],

