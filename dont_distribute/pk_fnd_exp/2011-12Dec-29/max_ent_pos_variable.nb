  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", 
     RowBox[{"1", "/", "2"}]}], ",", "2", ",", "0"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.534158680926589*^9, 3.534158689328233*^9}, {
  3.534158827789322*^9, 3.534158879505975*^9}, {3.534159163776157*^9, 
