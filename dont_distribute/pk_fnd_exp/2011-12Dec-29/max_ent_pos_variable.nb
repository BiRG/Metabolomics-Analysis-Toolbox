    SqrtBox["\[Pi]"], " ", 
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"Erf", "[", 
       FractionBox[
        RowBox[{"\[Lambda]1", "-", 
         RowBox[{"2", " ", "\[Lambda]2", " ", "\[Mu]"}]}], 
        RowBox[{"2", " ", 
         SqrtBox[
          RowBox[{"-", "\[Lambda]2"}]]}]], "]"}]}], ")"}]}]]}]], "Input",
 CellChangeTimes->{{3.534172608144945*^9, 3.5341726413939953`*^9}}],

Cell[CellGroupData[{

