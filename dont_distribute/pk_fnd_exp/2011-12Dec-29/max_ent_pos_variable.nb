     RowBox[{"\[Lambda]2", " ", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"x", "-", "\[Mu]"}], ")"}], "2"]}], "-", 
     RowBox[{"\[Lambda]1", " ", "\[Mu]"}]}]], " ", 
   SqrtBox[
    RowBox[{"-", "\[Lambda]2"}]]}], 
  RowBox[{
   SqrtBox["\[Pi]"], " ", 
   RowBox[{"(", 
    RowBox[{"1", "+", 
     RowBox[{"Erf", "[", 
      FractionBox[
       RowBox[{"\[Lambda]1", "-", 
        RowBox[{"2", " ", "\[Lambda]2", " ", "\[Mu]"}]}], 
       RowBox[{"2", " ", 
        SqrtBox[
         RowBox[{"-", "\[Lambda]2"}]]}]], "]"}]}], ")"}]}]]], "Output",
 CellChangeTimes->{3.534172538278154*^9}]
}, Open  ]],

Cell[BoxData[
 RowBox[{
  RowBox[{"pp", "[", "x_", "]"}], ":=", 
  FractionBox[
   RowBox[{"2", " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"x", " ", "\[Lambda]1"}], "+", 
      FractionBox[
       SuperscriptBox["\[Lambda]1", "2"], 
       RowBox[{"4", " ", "\[Lambda]2"}]], "+", 
      RowBox[{"\[Lambda]2", " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"x", "-", "\[Mu]"}], ")"}], "2"]}], "-", 
      RowBox[{"\[Lambda]1", " ", "\[Mu]"}]}]], " ", 
    SqrtBox[
