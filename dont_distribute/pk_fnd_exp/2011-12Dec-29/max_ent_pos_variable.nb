   RowBox[{"2", " ", "\[Lambda]2", " ", "\[Mu]"}]}], "/.", 
  RowBox[{"\[Lambda]2", "->", 
   RowBox[{"-", 
    FractionBox["1", 
     RowBox[{"\[Pi]", " ", 
      SuperscriptBox["\[Mu]", "2"]}]]}]}]}]], "Input",
 CellChangeTimes->{{3.534188886377636*^9, 3.534188937659416*^9}, {
  3.534188983542116*^9, 3.53418899515707*^9}, {3.5341890325414762`*^9, 
  3.534189063654841*^9}}],

