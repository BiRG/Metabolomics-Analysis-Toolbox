Cell["\<\
twoFuncX gives the x position of the largest maximum for a given x0\
\>", "Text",
 CellChangeTimes->{{3.533931167550418*^9, 3.5339312083154907`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"twoFuncX", "[", "x0_", "]"}], ":=", 
   RowBox[{"Which", "[", 
    RowBox[{
     RowBox[{"0", "\[LessEqual]", 
      RowBox[{"Abs", "[", "x0", "]"}], "<", 
      FractionBox["1", 
       RowBox[{"2", " ", 
        SqrtBox["3"]}]]}], ",", "0", ",", "\[IndentingNewLine]", "True", ",", 
     
     RowBox[{"Root", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"1", "-", 
         RowBox[{"8", " ", 
          SuperscriptBox["x0", "2"]}], "-", 
         RowBox[{"48", " ", 
          SuperscriptBox["x0", "4"]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"8", "+", 
            RowBox[{"32", " ", 
             SuperscriptBox["x0", "2"]}]}], ")"}], " ", 
          SuperscriptBox["#1", "2"]}], "+", 
         RowBox[{"16", " ", 
          SuperscriptBox["#1", "4"]}]}], "&"}], ",", "2"}], "]"}]}], 
    "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.53392849310681*^9, 3.533928553280837*^9}, {
   3.533928649232521*^9, 3.533928782433261*^9}, {3.533929027265663*^9, 
   3.533929028626852*^9}, 3.5339290817916193`*^9, {3.533931211868258*^9, 
   3.533931414168694*^9}, {3.5339842234176073`*^9, 3.5339842658606577`*^9}}],

Cell["\<\
