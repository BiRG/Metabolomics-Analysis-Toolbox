Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", "q", "}"}], ",", 
   RowBox[{
    RowBox[{
     RowBox[{"q", "[", "x_", "]"}], ":=", 
     RowBox[{"twoFunctions", "[", 
