Cell[CellGroupData[{
Cell["Investigate finding maxima for peak detection", "Title",
 CellChangeTimes->{{3.53391209812129*^9, 3.533912100213738*^9}, {
  3.533912180093606*^9, 3.533912190203043*^9}, {3.533983491235003*^9, 
  3.533983496431755*^9}}],

Cell[CellGroupData[{

Cell["Set up the basic functions", "Section",
 CellChangeTimes->{{3.533912194770575*^9, 3.533912198760934*^9}}],

Cell["\<\
f is the Lorentzian function/Cauchy PDF as defined in Paul Anderson's thesis. \
 This makes \[Gamma] the width at half-height, amp the amplitude of the mode, \
and x0 the location of the mode on the x axis.\
\>", "Text",
 CellChangeTimes->{{3.5339122025967712`*^9, 3.533912236576455*^9}, {
  3.5339124777692137`*^9, 3.5339125825075293`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", 
   RowBox[{"x_", ",", "amp_", ",", "\[Gamma]_", ",", "x0_"}], "]"}], ":=", 
  RowBox[{"amp", " ", 
   RowBox[{
    SuperscriptBox["\[Gamma]", "2"], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"4", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"x", "-", "x0"}], ")"}], "2"]}], "+", 
      SuperscriptBox["\[Gamma]", "2"]}], ")"}]}]}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.533902679441391*^9, 3.533902770659884*^9}, {
   3.533902977173728*^9, 3.5339029870913353`*^9}, 3.533912488624444*^9, {
   3.533912547289925*^9, 3.5339125556312838`*^9}}],

Cell["And it is, plotted on a graph", "Text",
 CellChangeTimes->{{3.533913048279598*^9, 3.533913057622779*^9}, {
  3.53398332874754*^9, 3.533983341039713*^9}}],

