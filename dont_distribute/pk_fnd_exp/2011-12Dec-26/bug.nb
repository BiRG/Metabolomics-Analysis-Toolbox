Notebook[{

Cell[CellGroupData[{
Cell["Definite Integration bug", "Title",
 CellChangeTimes->{{3.53391209812129*^9, 3.533912100213738*^9}, {
  3.533912180093606*^9, 3.533912190203043*^9}, {3.533914531270514*^9, 
  3.533914537169847*^9}}],

Cell[CellGroupData[{

Cell["Set up the basic functions", "Section",
 CellChangeTimes->{{3.533912194770575*^9, 3.533912198760934*^9}}],

Cell[TextData[StyleBox["You can see the bug directly if you go down to the \
\"here's the bug\" heading.",
 FontSlant->"Italic",
 FontColor->RGBColor[0.5, 0, 0.5]]], "Text",
 CellChangeTimes->{{3.5339145797925262`*^9, 3.533914595663356*^9}}],

Cell["\<\
f is the Lorentzian function/Cauchy PDF as defined in Paul Anderson's thesis. \
 This makes \[Gamma] the width at half-height, amp the amplitude of the mode, \
and x0 the location of the mode on the x axis.\
\>", "Text",
 CellChangeTimes->{{3.5339122025967712`*^9, 3.533912236576455*^9}, {
  3.5339124777692137`*^9, 3.5339125825075293`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", 
   RowBox[{"x_", ",", "amp_", ",", "\[Gamma]_", ",", "x0_"}], "]"}], ":=", 
  RowBox[{"amp", " ", 
   RowBox[{
    SuperscriptBox["\[Gamma]", "2"], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"4", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"x", "-", "x0"}], ")"}], "2"]}], "+", 
      SuperscriptBox["\[Gamma]", "2"]}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.533902679441391*^9, 3.533902770659884*^9}, {
   3.533902977173728*^9, 3.5339029870913353`*^9}, 3.533912488624444*^9, {
   3.533912547289925*^9, 3.5339125556312838`*^9}}],

Cell["\<\
d is the negative of the 2nd derivative of a unit height Lorentzian with unit \
width at half-height with its maximum at 0\
\>", "Text",
 CellChangeTimes->{{3.5339127410466146`*^9, 3.5339127590329657`*^9}, {
  3.5339127980135803`*^9, 3.5339128053617573`*^9}, {3.533912876063238*^9, 
  3.533912892469812*^9}, {3.533913154005472*^9, 3.533913155261794*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"d", "[", "x_", "]"}], ":=", 
  RowBox[{"Evaluate", "[", 
   RowBox[{"-", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"x", ",", "1", ",", "1", ",", "0"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.533902773206129*^9, 3.5339028192383337`*^9}, {
  3.533902991556563*^9, 3.533902995751248*^9}, {3.533903059379784*^9, 
  3.53390313674627*^9}, {3.533913298526793*^9, 3.533913310421773*^9}}],

Cell["Here you can see the formulas for d and the unit f", "Text",
 CellChangeTimes->{{3.533913031945335*^9, 3.533913043671582*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"f", "[", 
  RowBox[{"x", ",", "1", ",", "1", ",", "0"}], "]"}]], "Input",
 CellChangeTimes->{{3.533912904619265*^9, 3.533912938885991*^9}}],

Cell[BoxData[
 FractionBox["1", 
  RowBox[{"1", "+", 
   RowBox[{"4", " ", 
    SuperscriptBox["x", "2"]}]}]]], "Output",
 CellChangeTimes->{{3.533912909408986*^9, 3.533912939451487*^9}, 
   3.533914660573976*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"d", "[", "x", "]"}]], "Input",
 CellChangeTimes->{{3.533912762741809*^9, 3.533912763361165*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"-", 
   FractionBox[
    RowBox[{"128", " ", 
     SuperscriptBox["x", "2"]}], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"4", " ", 
        SuperscriptBox["x", "2"]}]}], ")"}], "3"]]}], "+", 
  FractionBox["8", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"4", " ", 
       SuperscriptBox["x", "2"]}]}], ")"}], "2"]]}]], "Output",
 CellChangeTimes->{{3.533912763749658*^9, 3.533912766956087*^9}, 
   3.533914662051794*^9}]
}, Open  ]],

Cell["And here they are, plotted together on the same graph", "Text",
